{
  "name": "RecrutingApp",
  "version": "1.0",
  "description": "Uses Google's Spreadsheet API",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "RecruitingApp",
    "default_popup": "popup.html"
  },
  "oauth2": {
    "client_id":"51934230420-43rpia2bfc88jeocv3l34b7cgifrqkva.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/drive"]
  },
  "permissions": [
        "identity",
        "tabs",
        "storage"
    ],
  "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
  "key": ""
}
